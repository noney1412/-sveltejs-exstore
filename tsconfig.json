{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"module": "NodeNext",
		"target": "ES6",
		"moduleResolution": "Node",
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": false,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"noImplicitAny": false,
		"isolatedModules": false,
		"suppressImplicitAnyIndexErrors": false,
		"types": [
			"vitest/globals",
			"@testing-library/jest-dom"
		],
		"paths": {
			"$examples": [
				"src/examples"
			],
			"$examples/*": [
				"src/examples/*"
			],
			"$lib": [
				"src/lib"
			],
			"$lib/*": [
				"src/lib/*"
			],
			"$routes": [
				"src/routes"
			],
			"$routes/*": [
				"src/routes/*"
			]
		}
	}
}