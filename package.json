{
	"name": "svelte-exstore",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev --host --mode development",
		"build": "vite build",
		"package": "svelte-package",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"test": "vitest --mode test",
		"test:ui": "vitest --ui --api.port=3300 --mode test",
		"open": "cypress open --e2e",
		"get-token": "node ./tokens/generate-design-token.cjs",
		"windi": "windicss-analysis"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "1.0.0-next.39",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "1.0.0-next.3",
		"@testing-library/dom": "^8.17.1",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/svelte": "^3.2.1",
		"@types/testing-library__jest-dom": "^5.14.5",
		"@typescript-eslint/eslint-plugin": "^5.36.1",
		"@typescript-eslint/parser": "^5.36.1",
		"@vitest/ui": "^0.22.1",
		"cypress": "^10.7.0",
		"eslint": "^8.23.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"jsdom": "^20.0.0",
		"local-cypress": "^1.2.6",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.54.8",
		"svelte": "^3.49.0",
		"svelte-check": "^2.9.0",
		"svelte-loader": "^3.1.3",
		"svelte-preprocess": "^4.10.7",
		"svelte-windicss-preprocess": "^4.2.8",
		"tslib": "^2.4.0",
		"typescript": "^4.8.2",
		"vite": "^3.0.9",
		"vite-plugin-windicss": "^1.8.7",
		"vitest": "^0.22.1",
		"windicss": "^3.5.6",
		"windicss-analysis": "^0.3.5"
	},
	"type": "module",
	"engines": {
		"node": ">=16.0.0"
	},
	"dependencies": {
		"immer": "^9.0.15"
	}
}
