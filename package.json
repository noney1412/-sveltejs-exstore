{
	"name": "svelte-exstore",
	"version": "2.0.14",
	"description": "A simple state management library for Svelte, able to connect with Redux DevTools",
	"keywords": [
		"svelte",
		"redux",
		"store"
	],
	"main": "index.js",
	"license": "MIT",
	"author": "Chanon Panpila",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/noney1412/svelte-exstore.git"
	},
	"bugs": {
		"url": "https://github.com/noney1412/svelte-exstore/issues"
	},
	"homepage": "https://github.com/noney1412/svelte-exstore",
	"scripts": {
		"dev": "vite dev --host --mode development",
		"build": "vite build",
		"package": "svelte-package",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"test": "vitest --mode test",
		"test:ui": "vitest --ui --api.port=3300 --mode test",
		"open": "cypress open --e2e",
		"get-token": "node ./tokens/generate-design-token.cjs",
		"windi": "windicss-analysis"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "^1.0.0-next.5",
		"@testing-library/dom": "^8.18.1",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/svelte": "^3.2.1",
		"@types/lodash": "^4.14.185",
		"@types/testing-library__jest-dom": "^5.14.5",
		"@typescript-eslint/eslint-plugin": "^5.38.0",
		"@typescript-eslint/parser": "^5.38.0",
		"@vitest/ui": "^0.22.1",
		"cypress": "^10.8.0",
		"eslint": "^8.23.1",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"jsdom": "^20.0.0",
		"local-cypress": "^1.2.6",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.55.0",
		"svelte": "^3.50.1",
		"svelte-check": "^2.9.0",
		"svelte-loader": "^3.1.3",
		"svelte-preprocess": "^4.10.7",
		"svelte-windicss-preprocess": "^4.2.8",
		"tslib": "^2.4.0",
		"typescript": "^4.8.3",
		"vite": "^3.1.3",
		"vite-plugin-vitest-typescript-assert": "^1.1.4",
		"vite-plugin-windicss": "^1.8.8",
		"vitest": "^0.22.1",
		"windicss": "^3.5.6",
		"windicss-analysis": "^0.3.5"
	},
	"type": "module",
	"dependencies": {
		"svelte-exstore": "^2.0.13"
	}
}
